# Blockchain RPC URLs
# Get free RPC URLs from: https://www.alchemy.com/ or https://infura.io/
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# OpenAI API Key (for AI-powered strategy generation)
# Optional - if not provided, template-based strategies will be used
# Get your key from: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=

# Avocado Multi-Sig Wallet Configuration
# Optional - only required if you want to execute transactions
# Leave empty to use VibeAgent in analysis-only mode
AVOCADO_WALLET_ADDRESS=
AVOCADO_API_ENDPOINT=https://api.instadapp.io/avocado

# Strategy Parameters
MIN_PROFIT_USD=50
MAX_GAS_PRICE_GWEI=100
MAX_TRANSACTION_VALUE_USD=10000
FLASH_LOAN_PROVIDER=aave_v3

# Autonomous Mode Configuration
AUTONOMOUS_MODE=false
REQUIRE_MANUAL_APPROVAL=true
SCAN_INTERVAL_SECONDS=60
ENABLED_NETWORKS=ethereum,polygon,arbitrum
ENABLED_DEXES=uniswap_v3,sushiswap

# Blacklisted addresses (comma-separated)
BLACKLISTED_ADDRESSES=

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=vibeagent.log

# Web Interface
PORT=5000
FLASK_PORT=5000
FLASK_DEBUG=false
